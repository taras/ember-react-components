{
  "name": "ember-react-components",
  "version": "2.0.2",
  "description": "React component support in Ember",
  "keywords": [
    "ember-addon",
    "decorator",
    "react"
  ],
  "repository": "https://github.com/alexlafroscia/ember-react-components",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "homepage": "https://alexlafroscia.com/ember-react-components",
  "scripts": {
    "build": "ember build",
